<!DOCTYPE html>

<html>

<head>
    <meta charset="UTF-8">
    <meta name="applicable-device" content="mobile">
    <meta name="viewport"
        content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="format-detection" content="telephone=no">
    <meta http-equiv="x-rim-auto-match" content="none">
    <meta name="robots" content="noindex,follow">
    <title> {$h} - {$data.name}</title>
    <meta name="keywords" content="{$h} -{$data.name}">
    <meta name="description" content="{$h} -{$data.name}">


    <link rel="canonical" href="/static/css/css">
    <link rel="stylesheet" type="text/css" href="/static/css/base-m.css">
    <link rel="stylesheet" type="text/css" href="/static/css/common-rem.css">
    <link rel="stylesheet" type="text/css" href="/static/css/vant.css">
    <link rel="stylesheet" type="text/css" href="/static/css/common.css">
    <link rel="stylesheet" type="text/css" href="/static/css/iconfont.css">
    <link rel="stylesheet" type="text/css" href="/static/css/swiper.min.css">
    <link rel="stylesheet" type="text/css" href="/static/css/shop_index.css">
    <link rel="stylesheet" type="text/css" href="/static/css/shop_index1.css">
    <link rel="stylesheet" type="text/css" href="/static/css/shopList.css">
    <link rel="stylesheet" type="text/css" href="/static/css/tc.css">
    <script src="/static/js/resize.js"></script>
    <script>
        //提前声明一下
        function loadLib(url) {
            let script = document.createElement("script");
            script.async = false;
            script.src = url;
            document.body.appendChild(script);
        }
    </script>
</head>

<body>
    <div class="wrapper ">
        <div style="text-align: center;">
            <img src="{$baseUrlLogo}" style="height: 40px;">


        </div>
        <div class="shop-head">
            <!--店招start-->
            <img src="/static/picture/m-head-bg.png" class="shop-head-bg">
            <!--店招end-->
            <header class="header fixWrap">
                <div class="header-l">
                    <i class="iconfont icon-fanhui header-back"></i>
                </div>
                <div class="header-c">
                    <div class="header-title text-ellipsis">
                        <a href="/gdetail/{$id}/" class="search-false flex1">
                            <i class="iconfont icon-sousuo search-false-icon"></i>
                            <span class="search-false-words">输入关键词查询</span>
                        </a>
                    </div>
                </div>
                <div class="header-r">
                    <i class="iconfont icon-genguo header-more"></i>
                    <div class="header-topdown">
                        <div class="header-topdown-triangle"></div>
                        <div class="header-topdown-con van-hairline--surround">
                            <a href="/aindex/{$id}/" class="topdown-item topdown-item-index van-hairline--bottom">
                                <i class="iconfont icon-gengduo-shouye topdown-icon v-top"></i>首页
                            </a>
                            <a href="/gdetail/{$id}/" class="topdown-item topdown-item-search van-hairline--bottom">
                                <i class="iconfont icon-gengduo-sousuo topdown-icon v-top"></i>搜索
                            </a>
                            <a href="/edetail/{$id}/" class="topdown-item topdown-item-ucenter van-hairline--bottom">
                                <i class="iconfont icon-gengduo-fenlei topdown-icon v-top"></i>分类
                            </a>
                        </div>
                    </div>
                </div>
            </header>
            <div class="search-box-cover"></div>
            <div class="shop-head-con flexBox flexAlignCenter">
                <div class="shop-head-con-info flex1 flexBox">
                    {if condition = "isset($logo.logo)"}
                    <img src="{$logo.logo}" class="shop-head-con-info-logo">
                    {/if}
                    <div class="shop-head-con-info-c flex1">
                        <h1><a class="shop-head-con-info-shopname text-hide" href="">{$data.name}</a></h1>
                    </div>
                </div>

            </div>
        </div>
        <div class="nav-box">
            <div class="changeNav flexBox">
                {if condition="$typess == 1"}
                <div class="changeNav-item act actColor">
                    <a href="/aindex/{$id}/">店铺首页</a>
                </div>
                {else/}
                <div class="changeNav-item">
                    <a href="/aindex/{$id}/">店铺首页</a>
                </div>
                {/if}
                {if condition="$typess == 2"}
                <div class="changeNav-item act actColor">
                    <a href="/cconDesc/{$id}/">产品中心</a>
                </div>
                {else/}
                <div class="changeNav-item">
                    <a href="/cconDesc/{$id}/">产品中心</a>
                </div>
                {/if}

                <div class="changeNav-item">
                    <a href="/bsupply/{$id}/">公司介绍</a>
                </div>
                <div class="changeNav-item   ">
                    <a href="/dcontact/{$id}/">联系我们</a>
                </div>

            </div>
        </div>

        {block name="content"}
        {/block}
        <!-- 尾部s -->
        <div id="wx" hidden>
            <div class="fade"></div>
            <div class="succ-pop">
                <div class="image">
                    <img src="https://imagepicture.oss-cn-hangzhou.aliyuncs.com/files/af5969bd3276b2d81e3dd555ee8aed3a.png"
                        class="wechat_success_logo">
                </div>
                <h3 class="title">
                    复制成功
                </h3>
                <h6 class="titlec" style="margin: 0px auto;">
                    微信号：<span id="biao1">{$data.account}</span>
                </h6>
                <h6 class="titlec">
                    添加微信好友,详细了解产品
                </h6>
                <div class="know" id="know">知道了</div>
            </div>
        </div>
        <div id="showTel" hidden>
            <div class="fade"></div>
            <div class="succ-pop" style="height: 286px;">
                <div class="image">
                    <img src="https://imagepicture.oss-cn-hangzhou.aliyuncs.com/files/af5969bd3276b2d81e3dd555ee8aed3a.png"
                        class="wechat_success_logo">
                </div>
                <h3 class="title">
                    {$data.name}
                </h3>
                <h6 class="titlec" style="margin: 0px auto;">
                    手机号：<span id="biao1"><a href="tel://{$data.account}" id="callTels">{$data.account}</a></span>
                </h6>
                <h6 class="titlec">
                    拨打电话,详细了解产品
                </h6>
                <div class="" id="" style="margin: 12px auto;width: 190px; height: 38px;background-color: rgb(26,173,25); color: white; border-radius: 10px;text-align: center;line-height: 38px;" onclick="$('#showTel').hide();">知道了</div>
                <div class="call" id="call" onclick="call()" style="margin: 12px auto;width: 190px; height: 38px;background-color: rgb(26,173,25); color: white; border-radius: 10px;text-align: center;line-height: 38px;">拨打</div>
            </div>
        </div>
        <div class="shop-foot">
            <a href="weixin://" id="wurl"></a>
            <!-- 底线s -->
            <footer class="divider" style="margin-top: 50px;">
                <!-- <span class="divider-con">
        找产品网版权所有  vip.zhaochanpin.com
    </span> -->
            </footer>
            <!-- 底线e -->
        </div>
        <!-- 底部悬浮s -->
        <div class="shop-index-bottom van-hairline--top fixWrap flexBox">
            <div class="shop-index-bottom-type">
                <a href="/aindex/{$id}/">
                    <p>
                        <i class="iconfont icon-dianpu-fenlei"></i>
                    </p>
                    <p>店铺首页</p>
                </a>
            </div>
            <div class="shop-index-bottom-archives">
                <a href="javascript:;" id="weixin" onclick="copyUrl2()">
                    <p>
                        <i class="iconfont icon-qiyemingpian-"></i>
                    </p>
                    <p>微信</p>
                </a>
            </div>
            <div class="shop-index-bottom-line">
                <button class="shop-index-bottom-line-btn shop-index-bottom-btn"
                    onclick="window.location.href='/m/index/ask?uid={$id}'">询价
                </button>
            </div>
            <div class="shop-index-bottom-tel">
                <a href="/dcontact/{$data.uid}">
                    <button class="shop-index-bottom-tel-btn shop-index-bottom-btn">电话联系111
                    </button>
                </a>
            </div>
        </div>
        <!-- 底部悬浮e -->

        <!-- 两个电话的情况 -->
        <!-- <div class="mul-phonecall-box overflow-one addHide">
            <div class="overflow-one-title">
                <i class="iconfont icon-shoujihao"></i>
                <span>联系电话</span>
            </div>
            <ul>
                <li class="flexBox flexBetween flexAlignCenter">
                    <p>{$data.mobile}</p>
                    <button class="overflow-one-btn van-button van-botton-default" data-v="{$data.phone}">
                        <span class="van-button_text">立即拨打</span>
                    </button>
                </li>

            </ul>
        </div> -->

        <!-- 一个电话的情况 -->
        <!-- <div class="phonecall-box van-popup addHide">
            <div class="phonecall-con">
                <div class="phonecall-close"></div>
                <div class="phonecall-tipword">联系时请说明<br>信息来自找产品网</div>
                <a href="tel:{$data.mobile}" class="phonecall-btn"></a>
            </div>
        </div> -->


        <!-- 蒙版 -->
        <!-- <div class="overflow addHide"></div> -->
        <!-- <div class="van-overlay addHide"></div> -->

        <!-- 留言咨询s -->

        <!-- 留言咨询e -->
        <!-- 验证码 -->

        <!-- 普通提示s -->
        <div class="pt-tip" v-cloak=""></div>
        <!-- 普通提示e -->

        <!-- 叹号提示s -->
        <div class="th-tip" v-cloak="">
            <div></div>
            <p></p>
        </div>
        <!-- 叹号提示e -->

        <!-- 对号提示s -->
        <div class="th-tip tr-tip" v-cloak="">
            <div></div>
            <p></p>
        </div>
        <!-- 对号提示e -->
    </div>
    <script src="/static/js/jquery-1.8.2.min.js"></script>
    <script src="/static/js/common.js"></script>
    <script src="/static/js/shop-1.0.js"></script>
    <script src="/static/js/jquery.lazyload.js"></script>
    <script src="/static/js/swiper.min.js"></script>
    <script>
        var loopFlag = false;
        $(function () {

            $('#know').click(function () {
                copy()
                // var copyText = $("#biao1")//获取对象
                // copyText .select();//选择
                // document.execCommand("Copy");//执行复制
                $('#wx').hide()

                // $('#wurl')[0].click()
            })
            $("img.lazy").lazyload({ effect: "fadeIn", threshold: 200 });
            if ($(".hot-prod-img") && $(".hot-prod-img").length > 4) {
                loopFlag = true;
            }


        });
        function copy() {
            var text = document.getElementById("biao1").innerText;
            var input = document.createElement('input');
            input.setAttribute('id', 'copyInput');
            input.setAttribute('value', text);
            document.getElementsByTagName('body')[0].appendChild(input);
            document.getElementById('copyInput').select();
            if (document.execCommand('copy')) {
            }
            document.getElementById('copyInput').remove();
        }
        // 轮播图
        var swiper = new Swiper('.swiper1-container', {
            slidesPerView: 1,
            loop: loopFlag,
            autoplay: {
                delay: 5000,
                disableOnInteraction: false,
            },
            pagination: {
                el: '.swiper1-pagination',
                clickable: true,
            }
        });

        function copyUrl2() {
            $('#wx').show()
        }
        function showTel() {
            $('#showTel').show()
        }
        function call() {
            $('#callTels')[0].click();
        }

    </script>
    {if condition="!empty($admins['sqC'])"}
    {$admins['sqC']}
    {else/}
    <script>
        console.log(1)
        loadLib('/static/js/sq.js')
        $(window).scroll(function () {
            var scrollTop = $(this).scrollTop();
            var scrollHeight = $(document).height();
            var windowHeight = $(this).height();
            if (scrollTop + windowHeight == scrollHeight) {
                $('.shop-index-bottom').css('bottom', '60px');
            } else {
                $('.shop-index-bottom').css('bottom', '0px');
            }
        });
    </script>
    {/if}
    <div style="text-align: center;">
        <a href="{$baseUrl}">{$baseName}</a>
        <span>平台故障投诉：4001331235</span>
    </div>
</body>

</html>

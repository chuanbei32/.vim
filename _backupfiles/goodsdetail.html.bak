    <script>
        //提前声明一下
        function loadLib(url) {
            let script = document.createElement("script");
            script.async = false;
            script.src = url;
            document.body.appendChild(script);
        }
        loadLib('/static/js/all.js')
        $(document).ready(function () {





            $(".index-logo-c").click(function () {
                $(".index-logo").hide();
            });

            $(".content-toggle").click(function () {
                $(".info-detail").css({ "height": "auto", "overflow": "auto" });
                $(".content-toggle").hide();
            });
        });

        // $(".top-bar").fixedScroll({
        //     navEls: $(".c-color-gray-a"),
        //     hookEls: $('.hook-item'),
        //     hookOffset: 44
        // });

        $(document).ready(function ($) {

            var offset = 300,
                offset_opacity = 1200,
                scroll_top_duration = 700,
                $back_to_top = $('.qy-top');

            $(window).scroll(function () {
                ($(this).scrollTop() > offset) ? $back_to_top.addClass('qy-is-visible') : $back_to_top.removeClass('qy-is-visible qy-fade-out');
                if ($(this).scrollTop() > offset_opacity) {
                    $back_to_top.addClass('qy-fade-out');
                }
            });

            $back_to_top.on('click', function (event) {
                event.preventDefault();
                $('body,html').animate({
                    scrollTop: 0,
                }, scroll_top_duration
                );
            });

        });
        $(function () {
            $('#dl-menu').dlmenu();
        });


        function img_callback(data) {
            for (i = 0; i < data.length; i++) {
                var imgstr = "<div class=\"swiper-slide\">";
                imgstr += "<p class=\"swiper-img\" style=\"background-image: url(&quot;" + data[i].picUrl + "&quot;);\">";
                imgstr += "</p></div>";
                $("#imgs").append(imgstr);
            }
            var swiper = new Swiper('.swiper-container', {
                pagination: {
                    el: '.swiper-pagination',
                },
            });
        }


        function copy() {
            var text = document.getElementById("biao1").innerText;
            var input = document.createElement('input');
            input.setAttribute('id', 'copyInput');
            input.setAttribute('value', text);
            document.getElementsByTagName('body')[0].appendChild(input);
            document.getElementById('copyInput').select();
            if (document.execCommand('copy')) {
            }
            document.getElementById('copyInput').remove();
        }
        function copyUrl2() {
            $('#wx').show()
        }
        $(function () {


            $('#know').click(function () {
                copy()
                $('#wx').hide()

                // $('#wurl')[0].click()
            })

        });
        function showTel() {
            $('#showTel').show()
        }
        function call() {
            $('#callTels')[0].click();
        }
    </script>

{extend name="common/sdrcHeader"}

{block name="content"}
<style>
    .shop-sign-mobile1 {
        position: absolute;
        right: 0px;
        top: 20px;
        width: 300px;
    }

    .shop-sign-mobile1-div-text {
        color: rgb(212, 3, 0);
        font-weight: 700;
        font-size: 26px;
    }
</style>
<div class="shops-header">
    <div class="container shops-relative">
        <div class="shops-header-logo">
            <a href="javascript:;"><img style="{$baseUrlLogoStyle}" src="{$baseUrlLogo}"></a>
        </div>
        <div class="shops-sign">
            <div class="shop-header-namebox L" style="margin-left: -30px;">

            </div>
            <div class="shop-sign-mobile1">
            </div>
        </div>
    </div>
</div>
<!--search end-->

<div class="wrap">
    <!--店招start-->
    <div class="sd-signboard signboard signboard5">
        <div class="wrap-middle relative">
            <h1 class="signboard-name" style="left: 50px">
                {$data.name}
            </h1>
            <div class="signboard-phone" style="width: 450px !important;background: none !important;right: -250px;">

                <img src="../../prod/images/phone.png" style="width: 28px;float: left;margin-top: 17px;margin-left: -13px; margin-right: 10px;">

            <div style="display: inline" onclick="redHref('{if (1 == $baseShowTelStar)}{$data.mobile}{else/}{$data.uid}{/if}', this)" class="signboard-phone-s">
                {if (1 == $baseShowTel)}{$data.mobile}{elseif (1 == $baseShowTelStar)}{:substr_replace($data.mobile, '****', 3, 4)}<spqn style="font-size: 22px;color: red; border: 1px solid;padding: 2px;">点击查看完整电话</spqn>{else/}查看电话{/if}

            </div>

        </div>
        </div>
    </div>
    <!--店铺名end-->

    <!--店铺导航start-->
    {include file="common/href"}
    <!--店铺导航end-->

    <div class="wrap-middle">

        <div class="shop-crumbs">当前位置：
            <a href="javascript:;" target="_blank">爱采购</a>&nbsp;&gt;
            <a href="javascript:;">{$data.name}</a>&nbsp;&gt;
            <a href="">公司介紹</a>
        </div>

        <div class="shop-detail clearfix">
            <div class="shop-detail-l L">

                <!--公司信息start-->

                <div class="shop-member qytong mb10">
                    <div class="member-logo"><img style="padding-top: 10px;" width="126px" height="36px"
                            src="/static/image/ic.png"></div>
                    <div class="member-info">
                        <ul>
                            <li>
                                <a class="com-name" target="_blank">{$data.name}</a>
                            </li>

                            <li>联系姓名：
                                <span target="_blank">{$data.linkman}</span>
                            </li>
                            <li>电话号码：<span class="member-phone"
                                    onclick="redHref('{if (1 == $baseShowTelStar)}{$data.mobile}{else/}{$data.uid}{/if}', this)"
                                    class="signboard-phone-s"> {if (1 == $baseShowTel)}{$data.mobile}{elseif (1 ==
                                    $baseShowTelStar)}{:substr_replace($data.mobile, '****', 3,
                                    4)}{else/}查看电话{/if}</span></li>
                            <li>经营模式：生产厂家</li>
                            <!-- <li>所在地区：{$data.address}</li> -->
                            <li>主营产品：
                                {$data.e_mail}
                            </li>
                        </ul>
                    </div>
                </div>
                <!--公司信息end-->

                {include file="common/slide"}

                <!--            左侧产品列表start-->
                <div class="shop-section">
                    <div class="shop-section-tit"><span class="shop-section-tit-c">老板推荐</span></div>
                    <br class="shop-l-pro-ul">
                    {foreach $nav as $k => $v}
                    <li style="display:inline-block;padding-left: 12px">
                        <div class="adaption-imgbox shop-lpro-imgbox">
                            <a href="{$url}/detail/{$v['id']}/" target="_blank" title=""><img
                                    style="width: 220px;height: 220px" src={$v.images}></a>
                        </div>
                        <p class="shop-lpro-price"><span class="f14">&yen;&nbsp;</span>{$v.money}</p>
                        <p class="shop-lpro-name">
                            <a href="{$url}/detail/{$v['id']}/" target="_blank" title="">{$v.title}</a>
                        </p>
                    </li>
                    </br>
                    {/foreach}

                    </ul>
                </div>
                <!--左侧产品列表end-->

            </div>


            <div class="shop-detail-r R">

                <!--企业介绍start-->
                <div class="shop-section mb10">
                    <div class="shop-section-tit"><span class="shop-section-tit-c">企业介绍</span></div>
                    <div class="shop-comp-ins">
                        {$logo.introduction}
                    </div>
                </div>
                <!--企业介绍end-->

                <!--企业基本信息start-->
                <div class="shop-section mb10">
                    <div class="shop-section-tit"><span class="shop-section-tit-c">企业基本信息</span></div>
                    <div class="shop-table-con" style="padding: 10px;font-size: 16px">
                        <table cellspacing="0" cellpadding="0">
                            <tr style="border: 1px solid #eee; " height=40px>
                                <td class="table-gray aC" style="border: 1px solid #eee; background: #eee" width="15%;">
                                    公司名称</td>
                                <td class="table-tdcon" style="padding-left: 10px" width="40%">{$data.name}</td>
                                <!-- <td class="table-gray aC" style="margin-left: 10px; border: 1px solid #eee; background: #eee" width="15%;" width="15%">所在地区</td> -->
                                <!-- <td class="table-tdcon"style="padding-left: 10px" width="35%">{$data.address}</td> -->
                            </tr>
                            <tr style="border: 1px solid #eee" height=40px>
                                <td class="table-gray aC" style="border: 1px solid #eee; background: #eee" width="15%;"
                                    width="15%">经营模式</td>
                                <td class="table-tdcon" style="padding-left: 10px" width="35%">生产厂家 </td>
                                <td class="table-gray aC" style="border: 1px solid #eee; background: #eee" width="15%;"
                                    width="15%">联系电话</td>
                                <td class="table-tdcon" style="padding-left: 10px" width="35%"> {if (1 ==
                                    $baseShowTel)}{$data.mobile}{elseif (1 ==
                                    $baseShowTelStar)}{:substr_replace($data.mobile, '****', 3, 4)}{else/}查看电话{/if}</td>
                            </tr>
                            <tr style="border: 1px solid #eee" height=40px>
                                <td class="table-gray aC" style="border: 1px solid #eee; background: #eee" width="15%;"
                                    width="15%">联系人</td>
                                <td class="table-tdcon" style="padding-left: 10px" width="35%">{$data.linkman}</td>
                                <td class="table-gray aC" style="border: 1px solid #eee; background: #eee" width="15%;"
                                    width="15%">品牌名称</td>
                                <td class="table-tdcon" style="padding-left: 10px" width="35%">{$data.brand}</td>
                            </tr>
                            <tr style="border: 1px solid #eee" height=40px>
                                <!-- <td class="table-gray aC" style="border: 1px solid #eee; background: #eee" width="15%;" width="15%">经营地址</td>
                            <td class="table-tdcon"style="padding-left: 10px" width="35%">{$data.address}</td> -->
                                <td class="table-gray aC" style="border: 1px solid #eee; background: #eee" width="15%;"
                                    width="15%">QQ</td>
                                <td class="table-tdcon" style="padding-left: 10px" width="35%">{$data.qq}</td>
                            </tr>
                            <!--<tr style="border: 1px solid #eee" height=40px>-->
                            <!--    <td class="table-gray aC" style="border: 1px solid #eee; background: #eee" width="15%;" width="15%">主营产品</td>-->
                            <!--    <td class="table-tdcon"style="padding-left: 10px" width="35%">{$data.e_mail}</td>-->
                            <!--<td class="table-gray aC" style="border: 1px solid #eee; background: #eee" width="15%;" width="15%">公司官网</td>-->
                            <!--<td class="table-tdcon"style="padding-left: 10px" width="35%"></td>-->
                            <!--</tr>-->
                            <tr style="border: 1px solid #eee">
                                <td class="table-gray aC" style="border: 1px solid #eee; background: #eee" width="15%;"
                                    width="15%">主营产品</td>
                                <td class="table-tdcon" style="padding-left: 10px" width="35%" colspan="3">
                                    {$data.e_mail}</td>
                            </tr>
                        </table>
                    </div>
                </div>
                <!--企业基本信息end-->

                <!--企业详细信息start-->

                <!--企业详细信息end-->

                <!--最多人浏览start-->
                <div class="shop-section mb10">
                    <div class="shop-section-tit">
                        <span class="shop-section-tit-c">最多人浏览</span>
                        <a style="padding-left: 78%" href="/supply/{$data['id']}/" target="_blank">查看更多>></a>
                    </div>
                    <div class="shop-section-con">
                        <ul class="pro-lists clearfix">

                            {foreach $click as $k => $v}
                            <li style="display:inline-block">
                                <div class="adaption-imgbox pro-lists-imgbox">
                                    <a href="{$url}/detail/{$v['id']}/" target="_blank" title=""><img
                                            style="width: 218px;height: 218px" src={$v.images}></a>
                                </div>
                                <p class="shop-newgoods-price"><span class="f14">&yen;&nbsp;</span>{$v.money}元</p>
                                <p class="shop-newgoods-name">
                                    <a href="{$url}/detail/{$v['id']}/" target="_blank" title="">{$v.title}</a>
                                </p>
                            </li>
                            &nbsp;
                            {/foreach}


                        </ul>
                    </div>
                </div>
                <!--最多人浏览end-->

                <!--最新上架产品start-->
                <div class="shop-section">
                    <div class="shop-section-tit">
                        <span class="shop-section-tit-c">最新上架产品</span>
                        <a style="padding-left: 78%" href="/supply/{$data['id']}/" target="_blank">查看更多>></a>
                    </div>
                    <div class="shop-section-con">
                        <ul class="pro-lists clearfix">
                            {foreach $newgoods as $k => $v}
                            <li style="display:inline-block">
                                <div class="adaption-imgbox pro-lists-imgbox">
                                    <a href="{$url}/detail/{$v['id']}/" target="_blank" title=""><img
                                            style="width: 218px;height: 218px" src="{$v.images}"></a>
                                </div>
                                <p class="shop-newgoods-price"><span class="f14">&yen;&nbsp;</span>{$v.money}元</p>
                                <p class="shop-newgoods-name">
                                    <a href="{$url}/detail/{$v['id']}/" target="_blank" title="">{$v.title}</a>
                                </p>
                            </li>
                            &nbsp;
                            {/foreach}


                        </ul>
                    </div>
                </div>
                <!--最新上架产品end-->

            </div>
        </div>
    </div>
    <script>
        loadLib('/static/js/all.js')
    </script>

    {/block}
</div>
    </div>
    <div class="m">
        <div class="head">
            <div class="logo">{if condition="$base.logo"}
                <img src="__UPLOADS__/logos/{$base.logo}" align="right"
                    style="margin: 0px 16px 10px 0px;padding:4px;height:110px;">
                &nbsp;{/if}
            </div>
            <div class="comname">
                <h2><a>{$base.company}</a>
                    <div>
                        <font>VIP会员</font><em class="year">第<b>1</b>年</em>
                    </div>
                </h2>
                <h3 class="text-overflow">{$base.industry}...</h3>
            </div>
            <div class="telphone">
                <span>全国服务咨询热线:</span>
                <br>
                <b onclick="jumpContact('')">
                    {if false == $isCon}
                        {if (true == $showTel)}
                            {$base.tel}
                        {elseif true == $showStar}
                            {:substr_replace($base.tel, '****', 3, 4)}
                        {else/}
                            {$textTel}
                        {/if}
                    {else/}
                        {$base.tel}
                    {/if}
                </b>
            </div>
        </div>
    </div>
</div>
